{
  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db",
    "MyCourseDbContextConnection": "DataSource=MyCourse.db"
  },
  "Courses": {
    "PerPage": 10,
    "InHome": 3,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": [
        "Id",
        "Title",
        "Rating",
        "CurrentPrice"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Time": {
    "Default": 60
  },
  "MemoryCache": {},
  "ResponseCache": {
    "Home": {
      "Duration": 60,
      "Location": "Client",
      "VaryByQueryKeys": [
        "page"
      ]
    }
  },
  "ImageOption": {
    "Default": 300
  }
}