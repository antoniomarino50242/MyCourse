{
    "ConnectionStrings": {
        "Default" : "Data Source=Data/MyCourse.db"
    },
    "Courses":{
        "PerPage": 10,
        "Order": {
            "By": "Rating",
            "Ascending": false,
            "Allow": ["Title", "Rating", "CurrentPrice"]
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "Time": {
        "Default": 60
    },
    "MemoryCache": {
        "SizeLimit": 1000
    },
    "ResponseCache": {
        "Home": {
            "Duration": 60,
            "Location": "Client",
            "VaryByQueryKeys": ["page"]
        }
    }
}